  "private": true,
  "scripts": {
    "getSchema": "source .env && get-graphql-schema -j=true $LUMINA_API_ENDPOINT > ./modules/admin/__schema__/schema.json",
    "dev": "NODE_OPTIONS=\"--max_old_space_size=4096\" yarn node app.lumina.local",
    "dev:windows": "set NODE_OPTIONS=\"--max_old_space_size=4096\"&&yarn node app.lumina.local",
    "build": "NODE_OPTIONS=\"--max_old_space_size=8192\" next build",
    "build:windows": "set NODE_OPTIONS=\"--max_old_space_size=8192\"&&next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --watchAll --runInBand"
  },
