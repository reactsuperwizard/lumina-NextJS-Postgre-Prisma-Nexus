{
  "name": "@lumina/api",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "prisma": "prisma",
    "generate": "yarn -s generate:prisma && yarn generate:artifacts && yarn generate:types && yarn generate:dist",
    "generate:windows": "yarn -s generate:prisma && yarn generate:artifacts:windows && yarn generate:types && yarn generate:dist:windows",
    "generate:artifacts": "yarn -s generate:git && yarn -s generate:nexus",
    "generate:artifacts:windows": "yarn -s generate:git && yarn -s generate:nexus:windows",
    "generate:git": "./scripts/build/git.sh",
    "generate:prisma": "prisma generate",
    "generate:nexus": "GEN_ARTIFACTS=true ts-node --project build.tsconfig.json --transpile-only src/schema",
    "generate:nexus:windows": "set GEN_ARTIFACTS=true&&ts-node --project build.tsconfig.json --transpile-only src/schema",
    "generate:types": "yarn graphql-codegen",
    "generate:dist": "rm -rf dist && tsc --project dist.tsconfig.json",
    "generate:dist:windows": "tsc --project dist.tsconfig.json",
    "dev": "concurrently \"yarn dev:typecheck\" \"yarn dev:run\"",
    "dev:windows": "concurrently \"yarn dev:typecheck\" \"yarn dev:run:windows\"",
    "dev:run": "GEN_ARTIFACTS=true ts-node-dev --transpile-only --project build.tsconfig.json src/server-local.ts",
    "dev:run:windows": "set GEN_ARTIFACTS=true&&ts-node-dev --transpile-only --project build.tsconfig.json src/server-local.ts",
    "dev:typecheck": "tsc --project build.tsconfig.json --watch --noEmit",
    "dev:debug": "ts-node-dev --inspect --transpile-only --project build.tsconfig.json src/server-local.ts",
    "deploy": "yarn generate && rm -rf .build &&  tsc --project build.tsconfig.json && serverless deploy --env production --stage production",
    "deploy:dev": "yarn generate && rm -rf .build &&  tsc --project build.tsconfig.json &&  serverless deploy --env dev --stage dev",
    "deploy:staging": "yarn generate && serverless deploy --env staging --stage staging",
    "deploy:staging:windows": "yarn generate:windows && serverless deploy --env staging --stage staging",
    "lint": "eslint src generated --ext .js,.ts",
    "postinstall": "prisma generate",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:debug": "mocha --inspect-brk --watch",
    "seedAssets": "node node_modules/.bin/ts-node ./scripts/seedAssets.ts",
    "replicateDb": "yarn dropDevDb && yarn dumpDb",
    "dumpDb": "pg_dump ${PROD_DB} | psql ${DEV_DB}",
    "dropDevDb": "psql ${DEV_DB} -c 'drop schema public cascade; create schema public;'",
    "syncDevDbToLocal": "node scripts/pullRemoteDb.ts",
    "migrate:dev": "prisma migrate dev --preview-feature"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sqs": "3.4.1",
    "@lumina/render": "*",
    "@prisma/client": "2.17.0",
    "@sendgrid/mail": "^7.4.0",
    "@slack/web-api": "^6.5.1",
    "@types/graphql-upload": "^8.0.7",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-lambda": "^3.5.0",
    "auth0": "^2.31.0",
    "better-logging": "^4.4.0",
    "chalk": "^4.1.0",
    "cloudinary": "1.23.0",
    "debug": "^4.2.0",
    "eventemitter3": "^4.0.4",
    "extend": "^3.0.2",
    "graphql": "15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-scalars": "^1.14.1",
    "graphql-shield": "^7.3.7",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "hashids": "^2.2.8",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "is-stream": "2.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "ms": "2.1.2",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.31.0",
    "node-fetch": "2.6.1",
    "permit": "^0.2.4",
    "vimeo": "^2.1.1",
    "webpack": "5.68.0",
    "superagent-proxy": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.17.10",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.2.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.20",
    "@types/node-fetch": "^2.5.4",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "apollo-server": "^3.5.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "graphql-cli": "^4.1.0",
    "husky": "^3.1.0",
    "mocha": "^7.1.1",
    "npm": "^6.14.7",
    "prisma": "2.17.0",
    "serverless": "^1.59.3",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-webpack": "^5.3.5",
    "sinon": "^9.2.2",
    "sls": "^0.1.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.1.1",
    "webpack-node-externals": "^2.5.2"
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
